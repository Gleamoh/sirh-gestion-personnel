<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>CollaborateurNavigatorController</servlet-name>
		<display-name>CollaborateurNavigatorController</display-name>
		<description></description>
		<servlet-class>dev.sgp.web.CollaborateurNavigatorController</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>CollaborateurProcessController</servlet-name>
		<display-name>CollaborateurProcessController</display-name>
		<description></description>
		<servlet-class>dev.sgp.web.CollaborateurProcessController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CollaborateurNavigatorController</servlet-name>
		<url-pattern>/collaborateurs/*</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>CollaborateurProcessController</servlet-name>
		<url-pattern>/collaborateurs-process/*</url-pattern>
	</servlet-mapping>
</web-app>
